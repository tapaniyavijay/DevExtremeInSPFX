{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "dev-extreme-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/devExtremeGrid/DevExtremeGridWebPart.js",
          "manifest": "./src/webparts/devExtremeGrid/DevExtremeGridWebPart.manifest.json"
        }
      ]
    },
    "voice-recording-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/voiceRecording/VoiceRecordingWebPart.js",
          "manifest": "./src/webparts/voiceRecording/VoiceRecordingWebPart.manifest.json"
        }
      ]
    },
    "voice-recording-tiny-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/voiceRecordingTiny/VoiceRecordingTinyWebPart.js",
          "manifest": "./src/webparts/voiceRecordingTiny/VoiceRecordingTinyWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DevExtremeGridWebPartStrings": "lib/webparts/devExtremeGrid/loc/{locale}.js",
    "VoiceRecordingWebPartStrings": "lib/webparts/voiceRecording/loc/{locale}.js",
    "VoiceRecordingTinyWebPartStrings": "lib/webparts/voiceRecordingTiny/loc/{locale}.js"
  }
}
